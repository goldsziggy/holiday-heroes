  "scripts": {
    "build:dev": "rollup -c rollup/dev.js",
    "build:prod": "rollup -c rollup/prod.js",
    "dev": "npm run build:dev && npm-run-all --parallel dev:watch dev:browse dev:server",
    "dev:browse": "browser-sync start --proxy 'http://localhost:8888' --files 'html/**/*.html, build/**/*.js' --no-notify",
    "dev:server": "babel-node src/server/index.js",
    "dev:watch": "onchange src -- npm run build:dev && dev:server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build:dev && babel-node src/server/index.js"
  },
